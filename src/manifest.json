{
	"name": "Just Let Me Go",
	"version": "0",
	"manifest_version": 2,
	"author": "Lyubomyr Shaydariv",
	"description": "Strips Google Click ID, UTM tracking parameters and bypasses tracking redirects from Evernote",
	"icons" :{
		"48" : "icon-48x48.png",
		"128" : "icon-128x128.png"
	},
	"background": {
		"scripts": [
			"modules.js",
			"modules/gclid.js",
			"modules/utm.js",
			"modules/evernote.js",
			"main.js"
		]
	},
	"permissions": [
		"http://*/*?*",
		"https://*/*?*",
		"webRequest",
		"webRequestBlocking"
	]
}
